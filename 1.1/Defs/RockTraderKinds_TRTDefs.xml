<?xml version="1.0" encoding="utf-8"?>
<Defs>
  <TraderKindDef>
    <defName>Caravan_Outlander_Excavation</defName>
    <label>earth movers</label>
    <commonality>1.5</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>750~1200</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Steel</thingDef>
        <countRange>450~650</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Plasteel</thingDef>
        <countRange>50~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Jade</thingDef>
        <countRange>50~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Gold</thingDef>
        <countRange>50~350</countRange>
      </li>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Uranium</thingDef>
        <countRange>50~350</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>TRTChunks</categoryDef>
        <price>Cheap</price>
        <thingDefCountRange>2~5</thingDefCountRange>
        <countRange>2~5</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>StoneChunks</categoryDef>
        <price>Cheap</price>
        <thingDefCountRange>2~4</thingDefCountRange>
        <countRange>3~5</countRange>
      </li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_Category">
        <categoryDef>Manufactured</categoryDef>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>BlocksSandstone</li>
          <li>BlocksLimestone</li>
          <li>BlocksGranite</li>
          <li>BlocksMarble</li>
          <li>BlocksSlate</li>
        </thingDefs>
      </li>
    </stockGenerators>
  </TraderKindDef>

  <TraderKindDef>
    <defName>Caravan_Neolithic_Excavation</defName>
    <label>rock dealers</label>
    <commonality>1.5</commonality>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>750~1200</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>TRTChunks</categoryDef>
        <thingDefCountRange>4~5</thingDefCountRange>
        <countRange>3~5</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>StoneBlocks</categoryDef>
        <thingDefCountRange>3~5</thingDefCountRange>
        <countRange>200~500</countRange>
      </li>
      <li Class="StockGenerator_Category">
        <categoryDef>StoneChunks</categoryDef>
        <price>Cheap</price>
        <thingDefCountRange>2~3</thingDefCountRange>
        <countRange>2~4</countRange>
      </li>
      <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
      <li Class="StockGenerator_Category">
        <categoryDef>Manufactured</categoryDef>
      </li>
      <li Class="StockGenerator_MultiDef">
        <thingDefs>
          <li>Steel</li>
          <li>Plasteel</li>
          <li>Gold</li>
          <li>Jade</li>
          <li>Uranium</li>
        </thingDefs>
      </li>
    </stockGenerators>
  </TraderKindDef>

  <TraderKindDef>
    <defName>Orbital_Excavation</defName>
    <label>excavation trader</label>
    <commonality>2</commonality>
    <orbital>true</orbital>
    <stockGenerators>
      <li Class="StockGenerator_SingleDef">
        <thingDef>Silver</thingDef>
        <countRange>1200~4000</countRange>
    </li>
    <li Class="StockGenerator_SingleDef">
      <thingDef>Jade</thingDef>
      <countRange>50~250</countRange>
    </li>
    <li Class="StockGenerator_SingleDef">
      <thingDef>Gold</thingDef>
      <countRange>50~250</countRange>
    </li>
    <li Class="StockGenerator_SingleDef">
      <thingDef>Uranium</thingDef>
      <countRange>50~250</countRange>
    </li>
    <li Class="StockGenerator_Category">
      <categoryDef>TRTChunks</categoryDef>
      <thingDefCountRange>5~15</thingDefCountRange>
      <countRange>20~65</countRange>
    </li>
    <li Class="StockGenerator_Category">
      <categoryDef>StoneBlocks</categoryDef>
      <thingDefCountRange>5~10</thingDefCountRange>
      <countRange>500~750</countRange>
    </li>
    <li Class="StockGenerator_BuyTradeTag"><tag>Art</tag></li>
    <li Class="StockGenerator_Category">
      <categoryDef>Manufactured</categoryDef>
    </li>
    <li Class="StockGenerator_MultiDef">
      <thingDefs>
        <li>Steel</li>
        <li>Plasteel</li>
      </thingDefs>
    </li>
  </stockGenerators>
</TraderKindDef>
</Defs>
